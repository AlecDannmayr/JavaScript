// If statment

var answer = window.confirm("Click Ok, get true. Click cancel, get false.");

if (answer === true) {
console.log("you said true!");
}
else { console.log ("You said false"); }

// Window.prompt — asks the user to add info into prompt field and return an answer.
// If statment (Watch out for odd quotes)


var answer = window.prompt("type Yes, No or Maybe. then click ok.");
if (answer === "Yes") {
console.log("You Said Yes!");
}
else if (answer === "Maybe") {
console.log("you said maybe. I don't know what to make of that!");
}
else if (answer === "No") {
console.log("you said no.");
            } else {
console.log("you rebel you");
}


// If - if
// Else If - if not if
// Else - if not if or else if

// Switch statement

var answer = window.prompt("Answer the Question");

switch (answer) {
    case "Yes":
    console.log("You said Yes");
    break;
    case "No":
    console.log("You said No");
    break;
}

switch (answer) {
    case "Yes":
    console.log("You said Yes");
    break;
    case "No":
    console.log("You said No");
    break;
    default: 
    console.log("Please Enter a Value");
    break;
}


// Multi statment

var whatWeWantFroChristmas = "Fish";
var whatWeGotForChristmas = "Puppy";

if (whatWeWantFroChristmas === whatWeGotForChristmas) {
    console.log("Yah, I love it!");
}
else {
    console.log("oh no, not good I got a cat")
    var notSoGood = true;
}

if (notSoGood) {
    console.log("Ahhh, maybe next year")
}

if (!notSoGood) {
    console.log("Wooowhooo!")
}






