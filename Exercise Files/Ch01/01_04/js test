// Regular expressions!

// Test method

// (Add I flag case insensitive)
// ^ look for cap at start of string!
// $ at end looks to see if caps appear at the end!


var string1 = "625386213";
var string2 = "76391234";
var string3 = "hello";
var string4 = "Hello";
var string5 = "87364123 Hello";

var regex = /ever\.$/I;

console.log(regex.test(string1));
console.log(regex.test(string2));
console.log(regex.test(string3));
console.log(regex.test(string4));
console.log(regex.test(string5));

